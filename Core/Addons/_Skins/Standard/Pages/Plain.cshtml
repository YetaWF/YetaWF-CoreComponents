@using global::YetaWF.Core.Pages
@using global::YetaWF.Core.Modules
@inherits global::YetaWF.Core.Pages.RazorPage
<!DOCTYPE html>
<html lang="@Manager.CurrentPage.GetPageLanguageId()">
<head>
    <meta http-equiv='Content-Type' content='text/html; charset=UTF-8' />
    <title></title>
</head>
<body class="@Manager.PageCss()">
    <noscript><div class="yDivWarning" style="height:100px;text-align:center;vertical-align:middle">This site requires Javascript</div></noscript>
    <div class="pageOuterWrapper">
        @(Html.RenderModuleAsync<global::YetaWF.Modules.TinyLogin.Modules.TinyLoginModule>().Result)
        @(Html.RenderModuleAsync<global::YetaWF.Modules.Menus.Modules.MainMenuModule>().Result)

        @RenderPaneAsync("", "MainPane AnyPane").Result

        <div class="pageFooter">[[PageOrSite,CopyrightEvaluated]]</div>

        @(Html.RenderPageControlAsync<global::YetaWF.Modules.PageEdit.Modules.PageControlModule>().Result)
    </div>
</body>
</html>
