@inherits YetaWF.Core.Views.Shared.PageSkin<YetaWF.Core.Skins.SkinDefinition>
@using YetaWF.Core.Support
<div id='@ControlId' class='yt_pageskin t_edit'>
    @Html.RenderHidden("AjaxUrl", YetaWFManager.UrlFor(typeof(YetaWF.Core.Controllers.Shared.SkinHelperController), "GetPageSkins"))
    <div class="t_collection">
        @Html.ExtLabelFor(m => Model.Collection)
        @Html.RenderSkinCollection("Collection", Model.Collection)
    </div>
    <div class="t_skin">
        @Html.ExtLabelFor(m => Model.FileName)
        @Html.RenderPageSkinsForCollection("FileName", Model.FileName, Model.Collection)
    </div>
</div>
<script>
    @using (DocumentReady()) {
        @: YetaWF_Template_PageSkin.pageInit('@ControlId');
    }
</script>