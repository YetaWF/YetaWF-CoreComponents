@inherits YetaWF.Core.Views.RazorView<YetaWF.Core.Modules.ModuleDefinition, object>
@using (CreateFormAsync("Edit_Partial", ActionName: ViewContext.RouteData.Values["action"] + "_Partial").Result) {
    @PartialForm(UseAreaViewName: false)
    @FormButtonsAsync(new FormButton[] {
        new FormButton() { ButtonType= ButtonTypeEnum.Submit, },
        new FormButton() { ButtonType= ButtonTypeEnum.Cancel, },
    }).Result;
}
